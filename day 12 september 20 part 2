#include <stdio.h>
int main() {
    int u, b = 0;
    printf("Enter the units consumed : ");
    scanf("%d", &u);
    if (u <= 0) {
        printf("no bills to pay!");
    }
    else if (u <= 100) {
        b = u * 5;
    }
    else if (u <= 200) {
        b = (100 * 5) + (u - 100) * 7;
    }
    else if (u <= 300) {
        b = (100 * 5) + (100 * 7) + (u - 200) * 10;
    }
    else {
        b = (100 * 5) + (100 * 7) + (100 * 10) + (u - 300) * 12;
    }
    if (u > 0) {
         printf("you have to pay : Rs %d\n", b);
    }
    return 0;
}
